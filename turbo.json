{
  "globalDotEnv": [".env"],
  "pipeline": {
    "dev": {
      "cache": false
    },
    "type-check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["type-check", "lint"],
      "outputs": ["build/**", "dist/**"]
    }
  }
}